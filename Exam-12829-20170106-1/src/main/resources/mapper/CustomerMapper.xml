<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.hand.mapper.CustomerMapper">
	<select id="selectByFistName" resultType="com.hand.entity.Customer" parameterType="com.hand.entity.Customer">
		select first_name  from  customer where first_name = #{firstName}
	</select>
</mapper>